{
    "env": {
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-plusplus": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off"
    }
}
